	partition dCalendario = calculated
		mode: import
		source =
				
				
				ADDCOLUMNS(
				
				CALENDAR(MIN(fVendas[data_venda]),max(fVendas[data_venda])),
				
				"Ano",FORMAT([Date],"yyyy"),
				
				"Mês Nome",FORMAT([Date],"mmm"),
				
				"Mês Num",MONTH([Date]),
				
				"Trimestre",FORMAT([Date],"q"),
				
				"Mês Ano",CONCATENATE(FORMAT([Date],"mmm "),FORMAT([Date],"yyyy")),
				
				"Dia Semana Ano",WEEKDAY([Date]),
				
				"Dia Mês", FORMAT([Date],"dd/MM"),
				
				"Mês Dia", FORMAT([Date],"MM/dd"),
				
				"Ano Mês",CONCATENATE(FORMAT([Date],"yyyy"),FORMAT([Date],"mmm")),
				
				"Primeiro dia Mes",date(YEAR([Date]),MONTH([Date]),1)
				
				)

	annotation PBI_Id = 859f568c87e24c3ab8eb4f06cd113a2e

